<h1>The Attendees</h1>
<table>

  <br/>
<%= gmaps4rails (@pins) %>
<%= yield :scripts %>

<thead>
    <tr>
      <th>Name</th>
      <th>Twitter handle</th>
      <th>Bio</th>
      <th>Address</th>
      <th>Picture</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @attendees.each do |attendee| %>
      <tr>
        <td><%= attendee.name %></td>
        <td><%= attendee.twitter_handle %></td>
        <td><%= attendee.bio %></td>
        <td><%= attendee.address %></td>
        <td><%= attendee.picture %></td>
        <td><%= link_to 'Show', attendee %></td>
        <td><%= link_to 'Edit', edit_attendee_path(attendee) %></td>
        <td><%= link_to 'Destroy', attendee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attendee', new_attendee_path %>
